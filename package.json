{
  "name": "libreosteo",
  "private": true,
  "version": "0.0.0",
  "description": "Libreosteo",
  "repository": "https://github.com/garthylou/Libreosteo",
  "license": "GNU-GPL",
  "devDependencies": {
    "karma": "~0.10",
    "protractor": "~0.20.1",
    "http-server": "^0.6.1",
    "bower": "^1.3.1",
    "shelljs": "^0.2.6",
    "karma-junit-reporter": "^0.2.2"
  },
  "scripts": {
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'libreosteoweb/static/bower_components', 'junction') } catch (e) { }\"",
    "prestart": "npm install",

    "pretest": "npm install",
    "test": "karma start test/karma.conf.js",
    "test-single-run": "karma start test/karma.conf.js  --single-run",

    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",

    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js",

    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + cat('libreosteoweb/bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'libreosteoweb/index-async.html');\""
  },
  "dependencies": {
    "@bower_components/angular": "angular/bower-angular#1.5.11",
    "@bower_components/angular-animate": "angular/bower-angular-animate#1.5.x",
    "@bower_components/angular-bind-html-compile" : "git://github.com/incuna/angular-bind-html-compile.git#1.1.0",
    "@bower_components/angular-bootstrap": "angular-ui/bootstrap-bower#2.5.x",
    "@bower_components/angular-cookies": "angular/bower-angular-cookies#1.5.x",
    "@bower_components/angular-daterangepicker": "fragaria/angular-daterangepicker#^0.2.2",
    "@bower_components/angular-growl": "marcorinck/angular-growl#*",
    "@bower_components/angular-i18n": "angular/bower-angular-i18n#1.5.x",
    "@bower_components/angular-loader": "angular/bower-angular-loader#1.5.x",
    "@bower_components/angular-loading-bar": "chieffancypants/angular-loading-bar#0.9.x",
    "@bower_components/angular-mocks": "angular/bower-angular-mocks#~1.5.x",
    "@bower_components/angular-resource": "angular/bower-angular-resource#1.5.x",
    "@bower_components/angular-route": "angular/bower-angular-route#1.5.x",
    "@bower_components/angular-sanitize": "angular/bower-angular-sanitize#1.5.x",
    "@bower_components/angular-scroll": "oblador/angular-scroll#*",
    "@bower_components/angular-timeago": "yaru22/angular-timeago#*",
    "@bower_components/angular-toArrayFilter": "petebacondarwin/angular-toArrayFilter#*",
    "@bower_components/angular-ui-grid": "angular-ui/bower-ui-grid#*",
    "@bower_components/angular-ui-router": "angular-ui/angular-ui-router-bower#*",
    "@bower_components/angular-ui-validate": "angular-ui/ui-validate#*",
    "@bower_components/angular-xeditable": "vitalets/angular-xeditable#*",
    "@bower_components/bootstrap": "twbs/bootstrap#^3.0.0",
    "@bower_components/bootstrap-daterangepicker": "dangrossman/bootstrap-daterangepicker#^2.0.0",
    "@bower_components/bootstrap-tour": "sorich87/bootstrap-tour#*",
    "@bower_components/font-awesome": "git://github.com/FortAwesome/Font-Awesome.git#~4.2.0",
    "@bower_components/hallo": "git://github.com/bergie/hallo.git#*",
    "@bower_components/jquery": "git://github.com/jquery/jquery-dist.git#1.9.x",
    "@bower_components/jquery-1.11.1": "jquery/jquery#1.11.3",
    "@bower_components/jquery-htmlclean": "git://github.com/components/jquery-htmlclean.git#1.3.x",
    "@bower_components/jquery-ui": "git://github.com/components/jqueryui.git#1.10.x",
    "@bower_components/jquery-ui-bootstrap": "git://github.com/gustavohenke/jquery-ui-bootstrap.git#*",
    "@bower_components/moment": "moment/moment#>=2.9.0",
    "@bower_components/ng-file-upload": "danialfarid/angular-file-upload-bower#*",
    "@bower_components/ngInfiniteScroll": "ng-infinite-scroll/ng-infinite-scroll-bower#*",
    "@bower_components/rangy": "git://github.com/timdown/rangy-release.git#*",
    "@bower_components/rangy-official": "git://github.com/timdown/rangy-release.git#1.3.x",
    "@bower_components/webshim": "aFarkas/webshim#*"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
